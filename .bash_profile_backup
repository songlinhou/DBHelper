# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/MacBook/anaconda2/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/MacBook/anaconda2/etc/profile.d/conda.sh" ]; then
        . "/Users/MacBook/anaconda2/etc/profile.d/conda.sh"
    else
        export PATH="/Users/MacBook/anaconda2/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

